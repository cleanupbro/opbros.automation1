# OpBros.Automation Environment Variables
# Copy this file to .env.local and fill in the values

# N8N Webhook for Lead Capture
NEXT_PUBLIC_N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/lead-capture

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://opbros.ai
NEXT_PUBLIC_SITE_NAME=OpBros.Automation

# Contact Information
NEXT_PUBLIC_CONTACT_EMAIL=contact@opbros.ai

# Analytics (Vercel)
NEXT_PUBLIC_VERCEL_ANALYTICS=true
